.bv-border-box-4 {
  position: relative;
  width: 100%;
  height: 100%;

  .bv-border-svg-container {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;

    & > polyline {
      fill: none;
    }
  }

  sw1() {
    stroke-width: 1;
  }

  sw3() {
    stroke-width: 3px;
    stroke-linecap: round;
  }

  .bv-bb4-line-1 {
    sw1();
  }

  .bv-bb4-line-2 {
    sw1();
  }

  .bv-bb4-line-3 {
    sw3();
  }

  .bv-bb4-line-4 {
    sw3();
  }

  .bv-bb4-line-5 {
    sw1();
  }

  .bv-bb4-line-6 {
    sw1();
  }

  .bv-bb4-line-7 {
    sw1();
  }

  .bv-bb4-line-8 {
    sw3();
  }

  .bv-bb4-line-9 {
    sw3();
    stroke-dasharray: 100 250;
  }

  .bv-bb4-line-10 {
    sw1();
    stroke-dasharray: 80 270;
  }

  .border-box-content {
    position: relative;
    width: 100%;
    height: 100%;
  }
}

